<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<ZXML>
    <ZAPP>
        <get_cmd_config>
            <CMD_DESC>获取当前ZAPP支持的WEB指令信息</CMD_DESC>
        </get_cmd_config>
        <get_app_config>
            <CMD_DESC>获取当前应用程序的配置信息</CMD_DESC>
        </get_app_config>
        <set_app_config>
            <CMD_DESC>设置当前应用程序配置信息</CMD_DESC>
            <CMD_PARAMS>
                <path>配置文件路径，使用ZXML_PATH格式：/AAA&lt;param1=123,paramb=456&gt;/BBB/.</path>
                <value>需要配置的配置文件的值</value>
            </CMD_PARAMS>
        </set_app_config>
        <delete_app_config>
            <CMD_DESC>删除指定应用程序配置信息</CMD_DESC>
            <CMD_PARAMS>
                <path>配置文件路径，使用ZXML_PATH格式：/AAA&lt;param1=123,paramb=456&gt;/BBB/.</path>
                <proc_attr>是否检测匹配属性：1或者0</proc_attr>
            </CMD_PARAMS>
        </delete_app_config>
        <get_app_info>
            <CMD_DESC>获取当前应用程序版本信息</CMD_DESC>
        </get_app_info>
    </ZAPP>
    <ZLOG>
        <get_log_config>
            <CMD_DESC>获取当前LOG系统配置</CMD_DESC>
        </get_log_config>
        <set_log_config>
            <CMD_DESC>设置当前LOG系统配置参数，实时生效</CMD_DESC>
            <CMD_PARAMS>
                <param>配置名称，不包括路径</param>
                <value>准备改变的值</value>
            </CMD_PARAMS>
        </set_log_config>
        <change_log_level>
            <CMD_DESC>改变指定LOG的输出等级，实时生效</CMD_DESC>
            <CMD_PARAMS>
                <src_info>指定的LOG源信息</src_info>
                <level>需要改变的等级</level>
            </CMD_PARAMS>
        </change_log_level>
        <list_log_session>
            <CMD_DESC>列出当前系统中存在的LOG会话</CMD_DESC>
        </list_log_session>
        <queue_log_realtime>
            <CMD_DESC>实时查询LOG信息</CMD_DESC>
            <CMD_PARAMS>
                <rec_count>查询的最大的记录数量</rec_count>
            </CMD_PARAMS>
        </queue_log_realtime>
        <queue_log_history>
            <CMD_DESC>使用SQL查询LOG数据库</CMD_DESC>
            <CMD_PARAMS>
                <session_name>需要查询的LOG会话名称，通过list_log_session获取</session_name>
                <sql>需要执行的sql指令</sql>
            </CMD_PARAMS>
        </queue_log_history>
        <get_zlog_memory_usage>
            <CMD_DESC>获取当前LOG系统内存使用情况，调试使用</CMD_DESC>
        </get_zlog_memory_usage>
    </ZLOG>
    <ZSYSINFO>
        <get_sys_info>
            <CMD_DESC>获取系统配置信息</CMD_DESC>
            <CMD_PARAMS>
                <class>获取系统信息的类别</class>
            </CMD_PARAMS>
        </get_sys_info>
        <list_sys_class>
            <CMD_DESC>获取系统配置信息类别列表</CMD_DESC>
        </list_sys_class>
    </ZSYSINFO>
    <ZMONITOR>
        <get_mon_config>
            <CMD_DESC>获取当前Monitor的配置信息</CMD_DESC>
        </get_mon_config>
        <get_mon_value>
            <CMD_DESC>获取当前Monitor的值</CMD_DESC>
            <CMD_PARAMS>
                <path>需要查询的路径，路径从/ZMON/起，不包括/ZMON/</path>
            </CMD_PARAMS>
        </get_mon_value>
    </ZMONITOR>
    <ZREGISTER>
        <list_reg_items>
            <CMD_DESC>列出所有的注册项</CMD_DESC>
        </list_reg_items>
        <get_reg_info>
            <CMD_DESC>查看注册项信息</CMD_DESC>
            <CMD_PARAMS>
                <reg_item>注册项名称</reg_item>
            </CMD_PARAMS>
        </get_reg_info>
    </ZREGISTER>
    <RTR>
        <ShareMemoryQueryHandler>
            <CMD_DESC>总体概览</CMD_DESC>
        </ShareMemoryQueryHandler>
        <QueryBrokenBlock>
            <CMD_DESC>查询所有坏快</CMD_DESC>
        </QueryBrokenBlock>
        <QueryFileBlock>
            <CMD_DESC>查询文件块信息</CMD_DESC>
            <CMD_PARAMS>
                <assetid>AssetID</assetid>
            </CMD_PARAMS>
        </QueryFileBlock>
        <ListAllProgram>
            <CMD_DESC>列出所有节目</CMD_DESC>
        </ListAllProgram>
        <QueryProgramCacheInfo>
            <CMD_DESC>查询节目cache信息</CMD_DESC>
            <CMD_PARAMS>
                <programid>ProgramID</programid>
            </CMD_PARAMS>
        </QueryProgramCacheInfo>
        <GetHelp>
            <CMD_DESC>帮助信息</CMD_DESC>
        </GetHelp>
    </RTR>
    <TrsClient>
        <PacketCount>
            <GetPacketCount>
                <CMD_DESC>获取丢包数量</CMD_DESC>
            </GetPacketCount>
        </PacketCount>
        <ClearPacketCount>
            <ClearCount>
                <CMD_DESC>清空丢包数量,重新计数</CMD_DESC>
            </ClearCount>
        </ClearPacketCount>
    </TrsClient>
</ZXML>
