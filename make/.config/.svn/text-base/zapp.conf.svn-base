<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<ZXML>
    <ZAPP>
        <HTTP_SERV>
            <PORT>7789</PORT>
            <SENDPORT>
                <UDPBROIP>168.1.255.255</UDPBROIP>
                <UDPBROPORT>6000</UDPBROPORT>
                <SENDTIME>5</SENDTIME>
            </SENDPORT>
        </HTTP_SERV>
    </ZAPP>
    <HELP>
        <ZAPP>
            <HTTP_SERV>
                <PORT>基于ZAPP的应用程序的HTTP网管端口号</PORT>
                <SENDPORT>
                    <UDPBROIP>向监控模块广播的广播地址</UDPBROIP>
                    <UDPBROPORT>向监控模块广播的广播端口</UDPBROPORT>
                    <SENDTIME>向监控模块广播的间隔时间</SENDTIME>
                </SENDPORT>
            </HTTP_SERV>
        </ZAPP>
        <ZTHREAD>
            <AUTO_FAULT_RESTART>是否启用线程异常自动重启功能,0:不启动;1:启动</AUTO_FAULT_RESTART>
        </ZTHREAD>
        <ZCOMM>
            <SMALL_MESSAGE_POOL_SIZE>小消息缓冲区大小，缺省选择：4096</SMALL_MESSAGE_POOL_SIZE>
            <LARGE_MESSAGE_POOL_SIZE>大消息缓冲区大小，缺省选择：256</LARGE_MESSAGE_POOL_SIZE>
            <MESSAGE_TRACE>
                <ENABLE>是否允许记录消息跟踪数据</ENABLE>
                <MAX_FILE_COUNT>最大的消息跟踪数据文件大小</MAX_FILE_COUNT>
                <MAX_MSG_COUNT_PER_FILE>每文件最大的保存消息数量</MAX_MSG_COUNT_PER_FILE>
                <MAX_MSG_BUFFER_COUNT>消息处理使用的消息缓冲大小</MAX_MSG_BUFFER_COUNT>
            </MESSAGE_TRACE>
            <USER_PROC_MSGQUEUE_SIZE>，用户消息处理队列长度，缺省选择：1024</USER_PROC_MSGQUEUE_SIZE>
            <CHECK_CHANNEL_TIMER>定时检测通道状态时间</CHECK_CHANNEL_TIMER>
            <SOCKET_POLICY>选择的通讯策略，0-&gt;thread，1-&gt;epoll，缺省选择：0</SOCKET_POLICY>
            <CHANNEL_SEND_MSGQUEUE_SIZE>，每个通道最大的发送队列消息数量，缺省选择：512</CHANNEL_SEND_MSGQUEUE_SIZE>
            <MSG_TIMEOUT_VALUE>消息超时时间，单位:秒</MSG_TIMEOUT_VALUE>
            <TEST_TIMEOUT_VALUE>发送测试消息超时时间，单位:秒</TEST_TIMEOUT_VALUE>
            <RECV_BUFFER_SIZE>SOCKET接收缓冲区配置，配置参数，必须大于最大消息长度，单位：BYTE</RECV_BUFFER_SIZE>
        </ZCOMM>
        <RTR_CONFIG>
            <SplitString>::或/</SplitString>
            <IndexPath>存储为MASS时，index存放路径</IndexPath>
            <StorePath>存储为本地时，存放路径</StorePath>
            <MaxFileCount>RTR支持的最大文件数量</MaxFileCount>
            <ShareCacheKey>Shm Key</ShareCacheKey>
            <EnableTouchFile>1写TouchFile，0不写</EnableTouchFile>
            <TouchFilePath>touch文件存放路径</TouchFilePath>
        </RTR_CONFIG>
        <Man>
            <IpAddress>Man服务器地址</IpAddress>
            <Port>Man服务器端口</Port>
            <ChannelId>通道ID</ChannelId>
        </Man>
        <Receiver>
            <count></count>
            <Receiver_0>
                <sendport></sendport>
                <resendport></resendport>
                <receiveport></receiveport>
                <sendaddr></sendaddr>
                <resendaddr></resendaddr>
                <receiveaddr></receiveaddr>
                <buffercount></buffercount>
                <fstype>0不写, 1写本地,2写MASS</fstype>
                <localaddr></localaddr>
                <enable_retransmit>是否允许重传，1允许，0禁止</enable_retransmit>
            </Receiver_0>
        </Receiver>
        <PriortyCDN>
            <CDNCount>CDN 数量</CDNCount>
            <CDN_0>
                <CDNIP>CDN Locate Ip</CDNIP>
                <CDNPort>CDN Locate Port</CDNPort>
                <ethCount>CDN 数量</ethCount>
                <eth0DeviceName>网卡设备名称</eth0DeviceName>
            </CDN_0>
            <EthCardThreadCount>netcard thread count</EthCardThreadCount>
        </PriortyCDN>
        <Transfer>
            <IngressCapacity>IngressCapacity 注入能力</IngressCapacity>
        </Transfer>
        <CheckNetCard>
            <CheckTime>检测网卡时间单位 ms</CheckTime>
        </CheckNetCard>
    </HELP>
    <ZTHREAD>
        <AUTO_FAULT_RESTART>1</AUTO_FAULT_RESTART>
    </ZTHREAD>
    <ZCOMM>
        <SMALL_MESSAGE_POOL_SIZE>4096</SMALL_MESSAGE_POOL_SIZE>
        <LARGE_MESSAGE_POOL_SIZE>256</LARGE_MESSAGE_POOL_SIZE>
        <MESSAGE_TRACE>
            <ENABLE>1</ENABLE>
            <MAX_FILE_COUNT>20</MAX_FILE_COUNT>
            <MAX_MSG_COUNT_PER_FILE>100000</MAX_MSG_COUNT_PER_FILE>
            <MAX_MSG_BUFFER_COUNT>10000</MAX_MSG_BUFFER_COUNT>
        </MESSAGE_TRACE>
        <USER_PROC_MSGQUEUE_SIZE>1024</USER_PROC_MSGQUEUE_SIZE>
        <CHECK_CHANNEL_TIMER>2</CHECK_CHANNEL_TIMER>
        <SOCKET_POLICY>0</SOCKET_POLICY>
        <CHANNEL_SEND_MSGQUEUE_SIZE>512</CHANNEL_SEND_MSGQUEUE_SIZE>
        <MSG_TIMEOUT_VALUE>15</MSG_TIMEOUT_VALUE>
        <TEST_TIMEOUT_VALUE>2</TEST_TIMEOUT_VALUE>
        <RECV_BUFFER_SIZE>4194304</RECV_BUFFER_SIZE>
    </ZCOMM>
    <RTR_CONFIG>
        <SplitString>/</SplitString>
        <IndexPath>/mnt/index</IndexPath>
        <StorePath>/mnt/css</StorePath>
        <MaxFileCount>600</MaxFileCount>
        <ShareCacheKey>100010</ShareCacheKey>
        <EnableTouchFile>1</EnableTouchFile>
        <TouchFilePath>/mnt/touch</TouchFilePath>
    </RTR_CONFIG>
    <Man>
        <IpAddress>0.0.0.0</IpAddress>
        <Port>5000</Port>
        <ChannelId>10</ChannelId>
    </Man>
    <Receiver>
        <count>1</count>
        <Receiver_0>
            <sendport>10001</sendport>
            <resendport>10002</resendport>
            <receiveport>10003</receiveport>
            <sendaddr>192.168.33.255</sendaddr>
            <resendaddr>192.168.33.255</resendaddr>
            <receiveaddr>192.168.33.1</receiveaddr>
            <buffercount>8000</buffercount>
            <fstype>1</fstype>
            <localaddr>192.168.33.1</localaddr>
            <enable_retransmit>0</enable_retransmit>
        </Receiver_0>
    </Receiver>
    <PriortyCDN>
        <CDNCount>1</CDNCount>
        <CDN_0>
            <CDNIP>127.0.0.1</CDNIP>
            <CDNPort>80</CDNPort>
            <ethCount>1</ethCount>
            <eth0DeviceName>eth0</eth0DeviceName>
        </CDN_0>
        <EthCardThreadCount>1</EthCardThreadCount>
    </PriortyCDN>
    <Transfer>
        <IngressCapacity>1000000000</IngressCapacity>
    </Transfer>
    <CheckNetCard>
        <CheckTime>500</CheckTime>
    </CheckNetCard>
</ZXML>
